{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://it-resonance-api.cfapps.us10-001.hana.ondemand.com/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "**",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, no-cache, must-revalidate" }
      ]
    }
  ]
}